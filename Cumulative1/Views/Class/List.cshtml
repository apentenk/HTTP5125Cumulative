@model IEnumerable<Cumulative1.Models.Course>
@{
    ViewBag.Title = "Class List";
}

<div id="containerList">
    <h2>List of Classes</h2>
    <form action="/Class/List" method="GET">
        <input type="text" name="SearchKey" autocomplete="off"/>
        <input type="submit" value="Search" />
    </form>
    @foreach(var course in Model)
            {
                <div class="listitem row">
                    <div class="col-md-12"`><a href="/Class/Show/@course.ClassId">@course.ClassCode @course.ClassName</a></div>
                </div>
            }
</div>

